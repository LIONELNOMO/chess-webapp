{
    "version": 2,
    "builds": [
        {
            "src": "**/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "css/**/*.css",
            "use": "@vercel/static"
        },
        {
            "src": "js/**/*.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/sicilian.html",
            "dest": "/sicilian.html"
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}
